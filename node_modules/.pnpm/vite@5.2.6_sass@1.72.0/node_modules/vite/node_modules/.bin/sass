#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/townyuan/Library/Mobile Documents/com~apple~CloudDocs/六角學院/_2023Vue直播班/Composition API/todolist/node_modules/.pnpm/sass@1.72.0/node_modules/sass/node_modules:/Users/townyuan/Library/Mobile Documents/com~apple~CloudDocs/六角學院/_2023Vue直播班/Composition API/todolist/node_modules/.pnpm/sass@1.72.0/node_modules:/Users/townyuan/Library/Mobile Documents/com~apple~CloudDocs/六角學院/_2023Vue直播班/Composition API/todolist/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/townyuan/Library/Mobile Documents/com~apple~CloudDocs/六角學院/_2023Vue直播班/Composition API/todolist/node_modules/.pnpm/sass@1.72.0/node_modules/sass/node_modules:/Users/townyuan/Library/Mobile Documents/com~apple~CloudDocs/六角學院/_2023Vue直播班/Composition API/todolist/node_modules/.pnpm/sass@1.72.0/node_modules:/Users/townyuan/Library/Mobile Documents/com~apple~CloudDocs/六角學院/_2023Vue直播班/Composition API/todolist/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sass@1.72.0/node_modules/sass/sass.js" "$@"
else
  exec node  "$basedir/../../../../../sass@1.72.0/node_modules/sass/sass.js" "$@"
fi
