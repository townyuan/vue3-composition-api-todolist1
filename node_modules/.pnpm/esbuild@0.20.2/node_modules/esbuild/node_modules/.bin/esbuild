#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/townyuan/Library/Mobile Documents/com~apple~CloudDocs/六角學院/_2023Vue直播班/Composition API/week1/vue-week1/node_modules/.pnpm/esbuild@0.20.2/node_modules/esbuild/bin/node_modules:/Users/townyuan/Library/Mobile Documents/com~apple~CloudDocs/六角學院/_2023Vue直播班/Composition API/week1/vue-week1/node_modules/.pnpm/esbuild@0.20.2/node_modules/esbuild/node_modules:/Users/townyuan/Library/Mobile Documents/com~apple~CloudDocs/六角學院/_2023Vue直播班/Composition API/week1/vue-week1/node_modules/.pnpm/esbuild@0.20.2/node_modules:/Users/townyuan/Library/Mobile Documents/com~apple~CloudDocs/六角學院/_2023Vue直播班/Composition API/week1/vue-week1/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/townyuan/Library/Mobile Documents/com~apple~CloudDocs/六角學院/_2023Vue直播班/Composition API/week1/vue-week1/node_modules/.pnpm/esbuild@0.20.2/node_modules/esbuild/bin/node_modules:/Users/townyuan/Library/Mobile Documents/com~apple~CloudDocs/六角學院/_2023Vue直播班/Composition API/week1/vue-week1/node_modules/.pnpm/esbuild@0.20.2/node_modules/esbuild/node_modules:/Users/townyuan/Library/Mobile Documents/com~apple~CloudDocs/六角學院/_2023Vue直播班/Composition API/week1/vue-week1/node_modules/.pnpm/esbuild@0.20.2/node_modules:/Users/townyuan/Library/Mobile Documents/com~apple~CloudDocs/六角學院/_2023Vue直播班/Composition API/week1/vue-week1/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esbuild" "$@"
else
  exec node  "$basedir/../../bin/esbuild" "$@"
fi
